{
  "name": "your-game-ui-vue",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "files": [
    "dist-lib"
  ],
  "main": "./dist-lib/your-game.umd.js",
  "module": "./dist-lib/your-game.es.js",
  "exports": {
    ".": {
      "import": "./dist-lib/your-game.es.js",
      "require": "./dist-lib/your-game.umd.js"
    },
    "./dist/style.css": "./dist-lib/your-game.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "vue-tsc --noEmit && vite build --config vite.lib.config.ts",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "pinia": "^3.0.2",
    "socket.io-client": "^4.8.3",
    "vue": "^3.5.16"
  },
  "devDependencies": {
    "@types/node": "^22.19.7",
    "@vitejs/plugin-vue": "^6.0.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vue-tsc": "^2.2.10"
  }
}
